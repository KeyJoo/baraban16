<?xml version='1.0' encoding='utf-8'?>
<widget id="ru.baraban16.android" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Baraban16</name>
    <description>
        Furs luxury Cleaning-service in Russian Federation, Ukraine &amp; Kazakhstan.
    </description>
    <author email="vladimir.pavlychev@osarts.ru" href="https://www.osarts.ru">
        Os Arts: Web, Design, Photo, Mobile.
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:vladimir.pavlychev@osarts.ru" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="^7.0.0" />
</widget>
