<?xml version='1.0' encoding='utf-8'?>
<widget id="ru.baraban16.android" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>Baraban16</name>
    <description>
        Furs luxury Cleaning-service in Russian Federation, Ukraine &amp; Kazakhstan.
    </description>
    <author email="vladimir.pavlychev@osarts.ru" href="https://www.osarts.ru">
        Os Arts: Web, Design, Photo, Mobile.
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:vladimir.pavlychev@osarts.ru" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
